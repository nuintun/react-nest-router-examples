{
  "name": "react-nest-router-examples",
  "version": "1.0.0",
  "description": "React nest router examples.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "start": "node server.js",
    "prod": "node tools/bin/webpack.config.prod.js",
    "dev": "nodemon tools/bin/webpack.config.dev.js",
    "report": "node tools/bin/webpack.config.prod.js --report",
    "format": "prettier --write . --ignore-path .prettierignore"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      ".swcrc.js",
      "tools/**/*",
      ".postcssrc.js",
      "app.config.js",
      "svgo.config.js",
      ".browserslistrc",
      "wwwroot/favicon.ico"
    ]
  },
  "dependencies": {
    "@swc/helpers": "^0.5.6",
    "antd": "^5.15.0",
    "core-js": "^3.36.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-nest-router": "^0.19.12"
  },
  "devDependencies": {
    "@nuintun/svgo-loader": "^0.2.5",
    "@swc/core": "^1.4.2",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "@types/throttle-debounce": "^5.0.2",
    "autoprefixer": "^10.4.18",
    "bufferutil": "^4.0.8",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "css-modules-types-loader": "^0.3.31",
    "find-free-ports": "^3.1.1",
    "html-webpack-plugin": "^5.6.0",
    "koa": "^2.15.0",
    "koa-compress": "^5.1.1",
    "koa-files": "^2.3.6",
    "memfs": "^4.7.7",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "svgc-loader": "^0.2.5",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.3.3",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-service": "^0.6.1"
  },
  "overrides": {
    "ws": "^8.0.0"
  },
  "resolutions": {
    "ws": "^8.0.0"
  }
}
